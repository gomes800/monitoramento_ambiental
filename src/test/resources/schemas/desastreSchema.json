{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "tipo": {
      "type": "string"
    },
    "localizacao": {
      "type": "string"
    },
    "severidade": {
      "type": "string"
    },
    "dataHora": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["id", "tipo", "localizacao", "severidade", "dataHora"]
}
